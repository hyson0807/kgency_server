# 서버 설정
PORT=5004
NODE_ENV=development

# Supabase 설정
KEY_1=your_supabase_url_here
KEY_2=your_supabase_anon_key_here

# JWT 설정
JWT_SECRET=your_jwt_secret_here

# SMS 인증 (Solapi)
SOLAPI_API_KEY=your_solapi_api_key_here
SOLAPI_API_SECRET=your_solapi_api_secret_here
SENDER_PHONE=your_sender_phone_here

# Google Translate API
GOOGLE_TRANSLATE_API_KEY=your_google_translate_api_key_here

# OpenAI API (선택사항)
OPENAI_API_KEY=your_openai_api_key_here

# Redis 설정 (Railway 배포용)
# Railway에서 Redis 애드온 추가 시 자동으로 설정됩니다
REDIS_PASSWORD=your_redis_password_here
REDIS_URL=redis://default:your_redis_password@your_redis_host:port

# Railway 배포 설정
RAILWAY_STATIC_URL=
RAILWAY_GIT_COMMIT_SHA=